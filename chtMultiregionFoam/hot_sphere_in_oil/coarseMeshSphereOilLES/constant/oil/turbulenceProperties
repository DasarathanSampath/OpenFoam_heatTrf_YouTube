simulationType  LES;

LES
{
    turbulence      on;

    LESModel        kOmegaSSTIDDES;
    

    printCoeffs     on;

    delta           IDDESDelta;

    IDDESDeltaCoeffs
    {
        hmax           maxDeltaxyzCubeRoot;
        maxDeltaxyzCubeRootCoeffs
        {
        }
    }
    Prt		0.9;


    LESModel	Smagorinsky;

    delta	cubeRootVol;
}


RAS
{
    RASModel            kOmegaSST;
    RASModel		kEpsilon;
    turbulence          on;

    printCoeffs         on;

    Prt			0.9;
}


